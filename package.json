{
  "name": "osaisen",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "node": "hardhat node",
    "dev:setup": "hardhat run scripts/local-setup.ts --network localhost",
    "deploy:local": "hardhat run scripts/deploy-local.ts --network localhost",
    "deploy:amoy": "hardhat run scripts/deploy-amoy.ts --network amoy",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network polygon",
    "test:userop": "npx ts-node scripts/test-userOp.ts",
    "metadata:generate": "npx ts-node scripts/generate-metadata.ts",
    "images:generate": "npx ts-node scripts/generate-images.ts",
    "images:upload": "npx ts-node scripts/upload-to-pinata.ts",
    "images:generate-and-upload": "npx ts-node scripts/generate-and-upload.ts",
    "metadata:upload-directory": "npx ts-node scripts/upload-metadata-directory.ts",
    "nft:set-uri": "hardhat run scripts/set-nft-baseuri.ts",
    "monitor:treasury": "hardhat run scripts/monitor-treasury.ts",
    "monitor:treasury:amoy": "hardhat run scripts/monitor-treasury.ts --network amoy -- --once",
    "monitor:treasury:polygon": "hardhat run scripts/monitor-treasury.ts --network polygon -- --once",
    "monitor:paymaster": "npx ts-node scripts/monitor-paymaster.ts --once",
    "monitor:paymaster:amoy": "npx ts-node scripts/monitor-paymaster.ts --network=amoy --once",
    "monitor:paymaster:polygon": "npx ts-node scripts/monitor-paymaster.ts --network=polygon --once"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.3",
    "@nomicfoundation/hardhat-ignition": "^0.15.16",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.17",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@nomicfoundation/ignition-core": "^0.15.15",
    "@openzeppelin/contracts": "^5.4.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "@types/sharp": "^0.31.1",
    "chai": "^4.5.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "hardhat": "^2.26.0",
    "hardhat-gas-reporter": "^2.3.0",
    "solidity-coverage": "^0.8.17",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@pinata/sdk": "^2.1.0",
    "permissionless": "^0.2.57",
    "sharp": "^0.34.5",
    "viem": "^2.21.0"
  }
}
